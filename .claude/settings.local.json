{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(*cargo *)",
      "Bash(*grep*)",
      "Bash(*rg*)",
      "Bash(mv:*)",
      "Bash(rm:*)",
      "Bash(direnv:*)",
      "Bash(rustc:*)",
      "Bash(rustup:*)",
      "Bash(mkdir:*)",
      "Bash(git:*)",
      "Bash(gh:*)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(cp:*)",
      "Bash(sed:*)",
      "Bash(cat:*)",
      "Bash(echo:*)",
      "Bash(just:*)",
      "Bash(wasm-pack:*)",
      "Bash(npm:*)",
      "Bash(node:*)",
      "Bash(trunk:*)",
      "Bash(dx:*)",
      "Bash(nix:*)",
      "Bash(timeout:*)",
      "Bash(curl:*)",
      "Bash(lastfm-edit:*)",
      "Bash(source:*)",
      "Bash(export:*)",
      "Bash(unset:*)",
      "Bash(env)",
      "Bash(exit)",
      "Bash(true)",
      "Bash(wait)",
      "Bash(kill:*)",
      "Bash(pkill:*)",
      "Bash(pgrep:*)",
      "Bash(chmod:*)",
      "Bash(bash:*)",
      "Bash(unzip:*)",
      "Bash(sips:*)",
      "Bash(iconutil:*)",
      "Bash(./target/*)",
      "Bash(./cli/target/*)",
      "Bash(./result/*)",
      "Bash(./*)",
      "Bash(for:*)",
      "Bash(do)",
      "Bash(done)",
      "Bash(if *)",
      "Bash(then)",
      "Bash(fi)",
      "Bash(/dev/null)",
      "Bash(/tmp:*)",
      "Bash(vcr-inspect:*)",
      "Bash(NIXPKGS_ALLOW_UNFREE=1 nix *)",
      "WebFetch(domain:docs.rs)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:crates.io)",
      "WebFetch(domain:dioxuslabs.github.io)",
      "WebFetch(domain:dioxuslabs.com)",
      "WebFetch(domain:musicbrainz.org)",
      "WebFetch(domain:en.wikipedia.org)",
      "Bash(RUST_LOG=debug cargo run -- music-brainz album-search --artist 'Sublime' --album 'Sublime' --limit 30)",
      "Bash(jq:*)",
      "Bash(cargo fmt:*)",
      "Bash(cargo clippy:*)",
      "Bash(cargo test:*)"
    ],
    "deny": [
      "Bash(dx serve:*)"
    ]
  }
}